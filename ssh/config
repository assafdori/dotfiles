Host *
  ForwardX11 no
  ForwardAgent no
  ServerAliveInterval 15
  ConnectTimeout 20
  MACs hmac-sha2-256,hmac-sha2-512

Host github.com
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/github

Host gitlab.com
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/gitlab

Host bitbucket.org
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/bitbucket
